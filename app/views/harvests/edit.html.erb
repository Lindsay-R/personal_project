<h2>Edit</h2>

<%= form_for @harvest do |f| %>
  <ul>
    <li>Location Of Your Plant <em>...Note: Click the ALLOW button to let the browser find your location.</em>

      <div id="city-state"></div>

      <section id="wrapper">
        <article>
        </article>
      </section>

      <div class="geo-container">
        <a href="#">Location Lookup</a>
        <%= f.hidden_field :latitude %>
        <%= f.hidden_field :longitude %>
        <%= f.hidden_field :city %>
        <%= f.hidden_field :state %>
      </div>
    </li>

    <br><br>

    <li>Produce Type
      <%= f.text_field :produce_type, placeholder: "Produce Type" %>
    </li>
    <br><br>

    <li>Image

      <%= f.text_field :image_name, placeholder: "image name"  %>

      <%= f.file_field :image%>
    </li>

    <br><br>
    <li><%= f.submit %></li>

  </ul>
<% end %>

